<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
           xsi:type="TaskPaneApp">
  <Id>cdf19592-a2fc-4b2e-96e8-fcbc050c9b18</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>LEVANT</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="LEVANT"/>
  <Description DefaultValue="A comprehensive PV design and simulation tool."/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://orhanelturk.github.io/Orhan3/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="TabLEVANTPV">
              <Label resid="CustomTab.Label"/>

              <!-- Group 1: Project Info -->
              <Group id="GroupProjectInfo">
                <Label resid="GroupProjectInfo.Label"/>
                <Control xsi:type="Button" id="InputSheetButton">
                  <Label resid="InputSheetButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="InputSheet.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="LoadsButton">
                  <Label resid="LoadsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Loads.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ProjectLocationButton">
                  <Label resid="ProjectLocationButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ProjectLocation.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="TariffsButton">
                  <Label resid="TariffsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Tariffs.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 2: Solar Resource Data -->
              <Group id="GroupSolarResourceData">
                <Label resid="GroupSolarResourceData.Label"/>
                <Control xsi:type="Button" id="ImportWeatherDataButton">
                  <Label resid="ImportWeatherDataButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ImportWeatherData.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="GetFromPVGISButton">
                  <Label resid="GetFromPVGISButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="GetFromPVGIS.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 3: Design -->
              <Group id="GroupDesign">
                <Label resid="GroupDesign.Label"/>
                <Control xsi:type="Button" id="SolarLayoutButton">
                  <Label resid="SolarLayoutButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="SolarLayout.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ConsumptionVsProductionButton">
                  <Label resid="ConsumptionVsProductionButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ConsumptionVsProduction.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ElectricalDesignButton">
                  <Label resid="ElectricalDesignButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ElectricalDesign.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 4: Simulation -->
              <Group id="GroupSimulation">
                <Label resid="GroupSimulation.Label"/>
                <Control xsi:type="Button" id="SimulationButton">
                  <Label resid="SimulationButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Simulation.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 5: Project Costs -->
              <Group id="GroupProjectCosts">
                <Label resid="GroupProjectCosts.Label"/>
                <Control xsi:type="Button" id="CAPEXButton">
                  <Label resid="CAPEXButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="CAPEX.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="OpexButton">
                  <Label resid="OpexButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Opex.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 6: Financial Model -->
              <Group id="GroupFinancialModel">
                <Label resid="GroupFinancialModel.Label"/>
                <Control xsi:type="Button" id="ModelInputsButton">
                  <Label resid="ModelInputsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ModelInputs.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="FinancialResultsButton">
                  <Label resid="FinancialResultsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="FinancialResults.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 7: Optimization -->
              <Group id="GroupOptimization">
                <Label resid="GroupOptimization.Label"/>
                <Control xsi:type="Button" id="CompareLandsButton">
                  <Label resid="CompareLandsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="CompareLands.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="CompareComponentsButton">
                  <Label resid="CompareComponentsButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="CompareComponents.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="CompareCountriesButton">
                  <Label resid="CompareCountriesButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="CompareCountries.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="OptimizeSolutionButton">
                  <Label resid="OptimizeSolutionButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="OptimizeSolution.Url"/>
                  </Action>
                </Control>
              </Group>

              <!-- Group 8: Outputs -->
              <Group id="GroupOutputs">
                <Label resid="GroupOutputs.Label"/>
                <Control xsi:type="Button" id="FinalReportButton">
                  <Label resid="FinalReportButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="FinalReport.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportReportPDFButton">
                  <Label resid="ExportReportPDFButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportReportPDF.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportReportWordButton">
                  <Label resid="ExportReportWordButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportReportWord.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportLayoutPDFButton">
                  <Label resid="ExportLayoutPDFButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportLayoutPDF.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportLayoutDXFButton">
                  <Label resid="ExportLayoutDXFButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportLayoutDXF.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportLayoutKMZButton">
                  <Label resid="ExportLayoutKMZButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportLayoutKMZ.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportSLDPDFButton">
                  <Label resid="ExportSLDPDFButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportSLDPDF.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExportSLDDXFButton">
                  <Label resid="ExportSLDDXFButton.Label"/>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="ExportSLDDXF.Url"/>
                  </Action>
                </Control>
              </Group>

            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
