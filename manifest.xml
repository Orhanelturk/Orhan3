<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>cdf19592-a2fc-4b2e-96e8-fcbc050c9b18</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="PVSheets"/>
  <Description DefaultValue="PVSheets"/>
  <IconUrl DefaultValue="https://orhanelturk.github.io/Orhan3/Levantlogo.png"/>
  <HighResolutionIconUrl DefaultValue="https://orhanelturk.github.io/Orhan3/Levantlogo.png"/>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://orhanelturk.github.io/Orhan3/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="ProjectInfo">
              <Label resid="CustomTab.Label"/>
              <!-- A. Project Info. Group 1 -->
              <Group id="ProjectInfo">
                <Label resid="ProjectInfo.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <!-- Input Sheet -->
                <Control xsi:type="Button" id="InputSheet">
                  <Label resid="InputSheet.Label"/>
                  <Supertip>
                    <Title resid="InputSheet.Label"/>
                    <Description resid="InputSheet.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="InputSheet.16x16"/>
                    <bt:Image size="32" resid="InputSheet.32x32"/>
                    <bt:Image size="80" resid="InputSheet.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>InputSheetPane</TaskpaneId>
                    <SourceLocation resid="InputSheet.Url"/>
                  </Action>
                </Control>
                <!-- Loads -->
                <Control xsi:type="Button" id="Loads">
                  <Label resid="Loads.Label"/>
                  <Supertip>
                    <Title resid="Loads.Label"/>
                    <Description resid="Loads.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Loads.16x16"/>
                    <bt:Image size="32" resid="Loads.32x32"/>
                    <bt:Image size="80" resid="Loads.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>LoadsPane</TaskpaneId>
                    <SourceLocation resid="Loads.Url"/>
                  </Action>
                </Control>
                <!-- ProjectLocation -->
                <Control xsi:type="Button" id="ProjectLocation">
                  <Label resid="ProjectLocation.Label"/>
                  <Supertip>
                    <Title resid="ProjectLocation.Label"/>
                    <Description resid="ProjectLocation.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="ProjectLocation.16x16"/>
                    <bt:Image size="32" resid="ProjectLocation.32x32"/>
                    <bt:Image size="80" resid="ProjectLocation.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ProjectLocationPane</TaskpaneId>
                    <SourceLocation resid="ProjectLocation.Url"/>
                  </Action>
                </Control>
              </Group>
               <!-- A. Project Info. Group 2 -->
              <Group id="CommandsGroup1">
                <Label resid="CommandsGroup1.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <!-- Input Sheet -->
                <Control xsi:type="Button" id="InputSheet">
                  <Label resid="InputSheet.Label"/>
                  <Supertip>
                    <Title resid="InputSheet.Label"/>
                    <Description resid="InputSheet.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="InputSheet.16x16"/>
                    <bt:Image size="32" resid="InputSheet.32x32"/>
                    <bt:Image size="80" resid="InputSheet.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>InputSheetPane</TaskpaneId>
                    <SourceLocation resid="InputSheet.Url"/>
                  </Action>
                </Control>
              </Group>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <!-- LEVANT Button Icons -->
        <bt:Image id="InputSheet.16x16" DefaultValue="https://orhanelturk.github.io/Orhan3/InputSheet16x16.png"/>
        <bt:Image id="InputSheet.32x32" DefaultValue="https://orhanelturk.github.io/Orhan3/InputSheet32x32.png"/>
        <bt:Image id="InputSheet.80x80" DefaultValue="https://orhanelturk.github.io/Orhan3/InputSheet80x80.png"/>
        
        <!-- Solar Layout Button Icons -->
        <bt:Image id="Loads.16x16" DefaultValue="https://orhanelturk.github.io/Orhan3/Loads16x16.png"/>
        <bt:Image id="Loads.32x32" DefaultValue="https://orhanelturk.github.io/Orhan3/Loads32x32.png"/>
        <bt:Image id="Loads.80x80" DefaultValue="https://orhanelturk.github.io/Orhan3/Loads80x80.png"/>

        <!-- Project Location Button Icons -->
        <bt:Image id="ProjectLocation.16x16" DefaultValue="https://orhanelturk.github.io/Orhan3/ProjectLocation16x16.png"/>
        <bt:Image id="ProjectLocation.32x32" DefaultValue="https://orhanelturk.github.io/Orhan3/ProjectLocation32x32.png"/>
        <bt:Image id="ProjectLocation.80x80" DefaultValue="https://orhanelturk.github.io/Orhan3/ProjectLocation80x80.png"/>
        
        <!-- General Icons -->
        <bt:Image id="Icon.16x16" DefaultValue="https://orhanelturk.github.io/Orhan3/Levantlogo.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://orhanelturk.github.io/Orhan3/Levantlogo.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://orhanelturk.github.io/Orhan3/Levantlogo.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://orhanelturk.github.io/Orhan3/commands.html"/>
        <bt:Url id="InputSheet.Url" DefaultValue="https://orhanelturk.github.io/Orhan3/InputSheet.html"/>
        <bt:Url id="Loads.Url" DefaultValue="https://orhanelturk.github.io/Orhan3/Loads.html"/>
        <bt:Url id="ProjectLocation.Url" DefaultValue="https://orhanelturk.github.io/Orhan3/ProjectLocation.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="CustomTab.Label" DefaultValue="PVSheets"/>
        <bt:String id="ProjectInfo.Label" DefaultValue="Project Info"/>
        <bt:String id="CommandsGroup1.Label" DefaultValue="Project Info1"/>
        <bt:String id="InputSheet.Label" DefaultValue="Inputs Sheet"/>
        <bt:String id="Loads.Label" DefaultValue="Loads"/>
        <bt:String id="ProjectLocation.Label" DefaultValue="Project Location"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="InputSheet.Tooltip" DefaultValue="Click to open LEVANT Task Pane"/>
        <bt:String id="Loads.Tooltip" DefaultValue="Click to open the Solar Layout tool in a task pane."/>
        <bt:String id="ProjectLocation.Tooltip" DefaultValue="Click to open the Solar Layout tool in a task pane."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
